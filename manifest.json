{
  "name": "ReWordle",
  "description": "ReWordle provides Wordle insights through its light yet powerful guessing algorithm!",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://www.nytimes.com/games/wordle/index.html"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "/images/rewordle16.png",
      "32": "/images/rewordle32.png",
      "48": "/images/rewordle48.png",
      "128": "/images/rewordle128.png"
    }
  },
  "icons": {
    "16": "/images/rewordle16.png",
    "32": "/images/rewordle32.png",
    "48": "/images/rewordle48.png",
    "128": "/images/rewordle128.png"
  }
}